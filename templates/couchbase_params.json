{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "adminUsername": {
            "value": "<%=couchbase_username%>"
        },
        "adminPassword": {
            "value": "<%=couchbase_password%>"
        },
        "serverNodeCount": {
            "value": <%=couchbase_server_nodes_count%>
        },
        "serverDiskSize": {
            "value": <%=couchbase_server_disk_size%>
        },
        "serverVersion": {
            "value": "<%=couchbase_server_version%>"
        },
        "syncGatewayNodeCount": {
            "value": <%=couchbase_sync_gw_nodes_count%>
        },
        "syncGatewayVersion": {
            "value": "<%=couchbase_sync_gw_version%>"
        },
        "vmSize": {
            "value": "<%=couchbase_vm_size%>"
        },
        "location": {
            "value": "<%=couchbase_location%>"
        },
        "license": {
            "value": "<%=couchbase_licence%>"
        }
    }
}