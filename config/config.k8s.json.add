    "mgmt_win_vm_nsg_name": "piptemplates-mgmt-vm-nsg",
    "mgmt_win_vm_nic_name": "piptemplates-mgmt-vm-nic",
    "mgmt_win_vm_disk_name": "piptemplates-mgmt-vm-disk",
